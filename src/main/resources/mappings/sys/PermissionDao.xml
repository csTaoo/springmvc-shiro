<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shitao.sys.dao.PermissionDao">

	<select id="getAllPermisson" resultType="com.shitao.sys.entity.Permission">
		SELECT
		p.id,
		p.category name
		FROM
		sys_permission p
	</select>

</mapper>